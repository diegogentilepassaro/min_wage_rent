.     replace qmon = qmon + 3 if quarter(dofq(year_quarter)) == 2
  3.     replace qmon = qmon + 6 if quarter(dofq(year_quarter)) == 3
  4.     replace qmon = qmon + 9 if quarter(dofq(year_quarter)) == 4
  5. 
.     g year_month = ym(year(dofq(year_quarter)), qmon)
  6.     format year_month %tm
  7.     drop year_quarter qmon
  8.     order year_month, after(countyfips)
  9. 
.     reshape wide estcount avgwwage emp, i(countyfips statefips year_month) j(
> industry) string

> load_shapefile <- function(instub, file){
+ 
+   spf <- read_sf(file.path(instub, file))  %>%
+     select(STATE, PLACE, NAME, LSAD) %>%
+     rename(statefips = STATE,
+            place_code = PLACE,
+            place_name = NAME,
+            place_type = LSAD)
+   spf <- spf[st_is_valid(spf),]
+ 
+   return(spf)
+ }
